<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.freshjuice.fl.dao.DemoDao">
	
	
	<select id="getById" parameterType="java.lang.String" resultType="java.util.Map">
		select nickName, pwd, realName from t_user where userId = #{value, jdbcType=VARCHAR}
	</select>
	
	<insert id="addDemo" parameterType="java.util.Map">
		insert into t_user(userId) values(#{userId, jdbcType=VARCHAR})
	</insert>
	
</mapper>